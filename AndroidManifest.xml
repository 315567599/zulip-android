<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.humbughq.mobile"
    android:versionCode="5"
    android:versionName="0.6.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.humbughq.mobile.ZulipApp"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        <activity
            android:name="com.humbughq.mobile.HumbugActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.humbughq.mobile.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.humbughq.mobile.LegalActivity"
            android:label="@string/title_activity_legal" >
        </activity>
    </application>

</manifest>